  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "docker:dev": "docker-compose -f docker-compose.local.yml up -d",
    "docker:stop": "docker-compose -f docker-compose.local.yml down",
    "docker:logs": "docker-compose -f docker-compose.local.yml logs -f app",
    "docker:clean": "docker-compose -f docker-compose.local.yml down -v && docker system prune -f",
    "docker:monitoring": "docker-compose -f docker-compose.local.yml --profile monitoring up -d"
  },
